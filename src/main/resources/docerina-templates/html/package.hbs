<!DOCTYPE HTML>
<html>

<head>
    <title>{{refinePackagePath this}}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="docerina-theme/css/main.css" />
    <!--[if lte IE 9]><link rel="stylesheet" href="docerina-theme/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="docerina-theme/css/ie8.css" /><![endif]-->
</head>

<body>
    <!-- Wrapper -->
    <div id="wrapper">

        <!-- Main -->
        <div id="main">
            <div class="inner">

                <!-- Header -->
                <header id="header">
                    <a href="index.html" class="logo"><strong>Ballerina</strong> API Documentation</a>
                </header>

                <!-- Banner -->
                <section id="banner">
                    <div class="content">
                        {{#hasGlobalVariables}}
                            <h2>Global variables of {{refinePackagePath this}} package</h2>
                            <p>
                            <table>
                                <tr>
                                    <th>Variable Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each globalVariables}}{{#isPublic}}
                                    <tr>
                                        <td id="{{name}}">{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{globalVarDescription}}</td>
                                    </tr>
                                {{/isPublic}}{{/each}}
                            </table>
                            </p>
                        {{/hasGlobalVariables}}
                        <!-- Enums -->
                        {{#hasEnums}}
                        <h2>Enums of {{refinePackagePath this}} package</h2>
                        {{#each enums}} {{currentObject this}}
                            {{#isPublic}}
                                <div style="padding-bottom: 30px;">
                                    <h3 id="{{name}}">public enum {{name}}</h3>
                                    <p>{{oneValueAnnotation "description"}}</p>

                                    {{#if enumerators}}
                                        <p>
                                            <b>Enumerators:</b>
                                        <table>
                                            <tr>
                                                <th>Name</th>
                                                <th>Description</th>
                                            </tr>
                                            {{#each enumerators}}
                                                <tr>
                                                    <td>{{name}}</td>
                                                    <td>{{fieldAnnotation this "field"}}</td>
                                                </tr>
                                            {{/each}}
                                        </table>
                                        </p>
                                    {{/if}}
                                </div>
                            {{/isPublic}}
                        {{/each}}
                        <p></p>
                        <br>{{/hasEnums}}
                        {{#hasFunctions}}
                        <!-- Functions -->
                        <h2>Functions of {{refinePackagePath this}} package</h2>
                        {{#each functions}} {{currentObject this}}
                        {{#isPublic}}
                        <div style="padding-bottom: 30px;">
                            <h3 id="{{name}}">
                                public function {{#if receiver/typeNode}}&lt;{{typeText receiver}} {{receiver/name}}&gt; {{/if}}{{name}}({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}}) {{#if returnParameters}}({{#each returnParameters}}{{typeText this}}{{parameterName name}}{{#unless @last}}, {{/unless}}{{/each}}){{/if}}
                            </h3>
                            <p>{{oneValueAnnotation "description"}}</p>

                            {{#if receiver}}
                            <p>
                                <b>Receiver:</b>
                                <table>
                                    <tr>
                                        <th>Receiver Name</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    <tr>
                                        <td>{{receiver/name}}</td><td><a href="{{#bindLink receiver/typeKind}}{{/bindLink}}"{{typeTitle receiver/typeKind}}>{{typeText receiver}}</a></td><td>{{paramAnnotation receiver "param"}}</td>
                                    </tr>
                                </table>
                            </p>
                            {{/if}}
                            {{#if parameters}}
                            <p>
                                <b>Parameters:</b>
                                <table>
                                    <tr>
                                        <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each parameters}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                            {{/if}}
                            {{#if returnParameters}}
                            <p>
                                <b>Return Parameters:</b>
                                <table>
                                    <tr>
                                        <th>Return Variable</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each returnParameters}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "return"}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                            {{/if}}
                        </div>
                        {{/isPublic}}
                        {{/each}}
                        <p></p>
                        <br>{{/hasFunctions}} 
                        {{#hasConnectors}}
                        <!-- Connectors -->
                        <h2>Connectors of {{refinePackagePath this}} package</h2>
                        {{#each connectors}} {{currentObject this}}
                        {{#isPublic}}
                        <h3 id="{{name}}">
                            public connector {{#with this}}{{this.name}} ({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/with}}
                        </h3>
                        <p>{{oneValueAnnotation "description"}}</p>
                        {{#if parameters}}
                        <p>
                            <b>Parameters:</b>
                            <table>
                                <tr>
                                    <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each parameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/if}}
                        <b>Actions:</b><br><br> {{#each actions}} {{currentObject this}}
                        <h3 id="{{name}}{{../name}}">action {{this.name}}({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}})</h3>
                        <p>{{oneValueAnnotation "description"}}</p>
                        {{#if parameters}}
                        <p>
                            <b>Parameters:</b>
                            <table>
                                <tr>
                                    <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each parameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/if}}
                        {{#if returnParameters}}
                        <p>
                            <b>Return Parameters:</b>
                            <table>
                                <tr>
                                    <th>Return Variable</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each returnParameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "return"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/if}} {{/each}} {{/isPublic}}{{/each}}
                        <br>{{/hasConnectors}} 
                        {{#hasStructs}}
                        <!-- Structs -->
                        <h2>Structs of {{refinePackagePath this}} package</h2>
                        {{#each structs}} {{currentObject this}}
                        {{#isPublic}}
                        <h3 id="{{name}}">public struct {{name}}</h3>
                        <p>{{oneValueAnnotation "description"}}</p>
                        {{#if fields}}
                        <p>
                            <b>Fields:</b>
                            <table>
                                <tr>
                                    <th>Field Name</th>
                                    <th>Data Type</th>
                                    <th>Description</th>
                                </tr>
                                {{#each fields}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{fieldAnnotation this "field"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/if}}
                        {{/isPublic}}{{/each}}<br> {{/hasStructs}}
                        {{#hasAnnotations}}
                        <!-- Annotations -->
                        <h2>Annotations of {{refinePackagePath this}} package</h2>
                        {{#each annotations}} {{currentObject this}}
                        <div style="padding-bottom: 30px;">
                            <h3 id="{{name}}">annotation {{name}}</h3>
                            <p>{{oneValueAnnotation "description"}}</p>

                            {{#if attributes}}
                            <p>
                                <b>Attributes:</b>
                                <table>
                                    <tr>
                                        <th>Attribute Name</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each attributes}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeNode}}{{/bindLink}}"{{typeTitle typeNode}}>{{annotationTypeText typeNode}}</a></td><td>{{annotFieldAnnotation}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                            {{/if}}
                        </div>
                        {{/each}}
                        <p></p>
                        <br>{{/hasAnnotations}}
                        {{#hasServices}}
                            <!-- Services -->
                            <h2>Services of {{refinePackagePath this}} package</h2>
                            {{#each services}} {{currentObject this}}
                                    <h3 id="{{name}}">
                                        service {{#with this}}{{this.name}}{{/with}}
                                    </h3>
                                    <p>{{oneValueAnnotation "description"}}</p>
                                    <b>Resources:</b><br><br> {{#each resources}} {{currentObject this}}
                                    <h3 id="{{name}}{{../name}}">resource {{this.name}}({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}})</h3>
                                    <p>{{oneValueAnnotation "description"}}</p>
                                    {{#if parameters}}
                                        <p>
                                            <b>Parameters:</b>
                                        <table>
                                            <tr>
                                                <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                            </tr>
                                            {{#each parameters}}
                                                <tr>
                                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                                </tr>
                                            {{/each}}
                                        </table>
                                        </p>
                                    {{/if}}
                                    {{#if returnParameters}}
                                        <p>
                                            <b>Return Parameters:</b>
                                        <table>
                                            <tr>
                                                <th>Return Variable</th><th>Data Type</th><th>Description</th>
                                            </tr>
                                            {{#each returnParameters}}
                                                <tr>
                                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText this}}</a></td><td>{{paramAnnotation this "return"}}</td>
                                                </tr>
                                            {{/each}}
                                        </table>
                                        </p>
                                    {{/if}} {{/each}} {{/each}}
                            <br>{{/hasServices}}
                        </div>
                </section>
            </div>
        </div>

        <!-- Sidebar -->
        <div id="sidebar">
            <div class="inner">

                <!-- Menu -->
                <nav id="menu" style="overflow-x: auto">
                    <header class="major">
                        <h2>Menu</h2>
                    </header>
                    <ul>
                        {{#hasGlobalVariables}}
                            <li>
                                <span class="opener">Global Variables</span>
                                <ul>
                                    {{#each globalVariables}}
                                        <li><a href="#{{name}}">{{name}}</a></li>
                                    {{/each}}
                                </ul>
                            </li>
                        {{/hasGlobalVariables}}
                        {{#hasEnums}}
                            <li>
                                <span class="opener">Enums</span>
                                <ul>
                                    {{#each enums}}
                                        <li><a href="#{{name}} ">{{name}}</a></li>
                                    {{/each}}
                                </ul>
                            </li>{{/hasEnums}}
                        {{#hasFunctions}}
                        <li>
                            <span class="opener">Functions</span>
                            <ul>
                            {{#each functions}}{{#isPublic}}
                                <li><a href="#{{name}}">{{#if receiver/typeNode}}&lt;{{typeText receiver}}&gt; {{/if}}{{name}}({{#each parameters}}{{typeText this}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}) {{#if returnParameters}}({{#each returnParameters}}{{typeText this}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/if}}</a>
                                </li>
                            {{/isPublic}}{{/each}}
                            </ul>
                        </li>
                        {{/hasFunctions}} 
                        {{#hasConnectors}}
                        <li>
                            <span class="opener">Connectors</span>
                            <ul>
                                {{#each connectors}}{{#isPublic}}
                                <li><a href="#{{this.name}} ">{{#with this}}{{this.name}} ({{#each parameters}}{{typeText this}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/with}}</a>
                                    <ul>
                                        {{#each actions}}
                                        <li><a href="#{{name}}{{../name}}">{{this.name}}({{#each parameters}}{{typeText this}} {{name}}{{#unless @last}}, {{/unless}}{{/each}})</a></li>
                                        {{/each}}
                                    </ul>
                                </li>{{/isPublic}}{{/each}}
                            </ul>
                        </li>{{/hasConnectors}} {{#hasStructs}}
                        <li>
                            <span class="opener">Structs</span>
                            <ul>
                                {{#each structs}}{{#isPublic}}
                                    <li><a href="#{{name}} ">{{name}}</a></li>
                                {{/isPublic}}{{/each}}
                            </ul>
                        </li>{{/hasStructs}} {{#hasAnnotations}}
                        <li>
                            <span class="opener">Annotations</span>
                            <ul>
                                {{#each annotations}}
                                    <li><a href="#{{name}} ">{{name}}</a></li>
                                {{/each}}
                            </ul>
                        </li>{{/hasAnnotations}}{{#hasServices}}
                        <li>
                            <span class="opener">Services</span>
                            <ul>
                                {{#each services}}
                                    <li><a href="#{{this.name}} ">{{#with this}}{{this.name}}{{/with}}</a>
                                        <ul>
                                            {{#each resources}}
                                                <li><a href="#{{name}}{{../name}}">{{this.name}}({{#each parameters}}{{typeText this}} {{name}}{{#unless @last}}, {{/unless}}{{/each}})</a></li>
                                            {{/each}}
                                        </ul>
                                    </li>{{/each}}
                            </ul>
                        </li>{{/hasServices}}
                    </ul>
                </nav>

                <!-- Footer -->
                <footer id="footer">
                    <p class="copyright">Copyright 2017 Ballerina API Documentation</p>
                </footer>
            </div>
        </div>
    </div>

    <!-- Scripts -->
    <script src="docerina-theme/js/jquery.min.js"></script>
    <script src="docerina-theme/js/skel.min.js"></script>
    <script src="docerina-theme/js/util.js"></script>
    <!--[if lte IE 8]><script src="docerina-theme/js/ie/respond.min.js"></script><![endif]-->
    <script src="docerina-theme/js/main.js"></script>
</body>
</html>
